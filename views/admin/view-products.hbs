<div class="container">


    <section class="mt-5">
        <div class="contaner">
            <div class="row">
                <a href="/admin/add-product" class="btn btn-success ml-auto">Add Products</a>
            </div>

        </div>




        <table class="table mt-4">
            <thead class="bg-success">
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Title</th>
                    <th scope="col">Category</th>
                    <th scope="col">image</th>
                    <th scope="col">Price</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                <tr>
                    <th scope="row">{{@index }}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.category}}</td>
                    <td><img style="width: 50px;height:50px" src="{{this.imageUrl}}" alt=""></td>
                    <td>{{this.price}}</td>
                </tr>
                {{/each}}

            </tbody>
        </table>
    </section>
</div>