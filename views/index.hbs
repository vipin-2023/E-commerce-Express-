
    <section>
        <div class="row justify-content-center">
            {{#each products}}

            <div class="card " style="width: 18rem;">
                <img class="card-img-top"
                    src="{{this.imageUrl}}"
                    alt="Card image cap" />
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.discription}}</p>
                    <a href="#" class="btn btn-primary">Add Cart</a>
                </div>
            </div>
            {{/each}}
           
        </div>
    </section>